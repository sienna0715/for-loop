{
  "name": "for-loop-icons",
  "private": false,
  "version": "0.1.1",
  "module": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tsc-build": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json && cp -r ./src/assets ./lib/cjs/src && cp -r ./src/assets ./lib/esm/src"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/sienna0715/for-loop-icons"
  },
  "keywords": [
    "react-icon-library",
    "react-component-library",
    "type-check"
  ],
  "author": {
    "name": "sienna0715 & lapmu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sienna0715/for-loop-icons/issues"
  },
  "homepage": "https://forloop.vercel.app/",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.28",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
